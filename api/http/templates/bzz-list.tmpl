<h3 class="top-space">Swarm index of {{ .URI }}</h3>
<hr>
<table>
	<thead>
		<tr>
			<th>Path</th>
			<th>Type</th>
			<th>Size</th>
		</tr>
	</thead>

	<tbody>
		{{ range .List.CommonPrefixes }}
		<tr>
			<td>
				<a class="normal-link" href="{{ basename . }}/">{{ basename . }}/</a>
			</td>
			<td>DIR</td>
			<td>-</td>
		</tr>
		{{ end }}
		{{ range .List.Entries }}
		<tr>
			<td>
				<a class="normal-link" href="{{ leaflink $.URI . }}">{{ basename .Path }}</a>
			</td>
			<td>{{ .ContentType }}</td>
			<td>{{ .Size }}</td>
		</tr>
		{{ end }}
</table>
<hr>